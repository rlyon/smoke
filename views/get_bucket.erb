<ListBucketResult xmlns="http://doc.s3.amazonaws.com/2006-03-01">
  <Name><%= @bucket.name %></Name>
  <Prefix><%= @prefix %></Prefix>
  <Marker></Marker>
  <MaxKeys>100</MaxKeys>
  <IsTruncated>false</IsTruncated>
  <% @objects.each do |object| %>
  <Contents>
    <Key><%= object.basename %></Key>
    <LastModified><%= object.updated_at.to_z %></LastModified>
    <ETag><%= object.etag %></ETag>
    <Size><%= object.size %></Size>
    <StorageClass><%= object.storage_class.upcase %></StorageClass>
    <Owner>
      <ID><%= object.user.access_id %></ID>
      <DisplayName><%= object.user.display_name %></DisplayName>
    </Owner>
  </Contents>
  <% end %>
  <% @common_prefixes.each do |prefix| %>
  <CommonPrefixes>
    <Prefix><%= prefix %></Prefix>
  </CommonPrefixes>
  <% end %>
</ListBucketResult>